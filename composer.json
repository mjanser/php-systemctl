{
    "name": "mjanser/systemctl",
    "type": "library",
    "description": "Library for managing systemd services using systemctl",
    "keywords": ["systemd"],
    "homepage": "https://github.com/mjanser/php-systemctl",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Martin Janser",
            "email": "martin@duss-janser.ch"
        }
    ],
    "support": {
        "source": "https://github.com/mjanser/php-systemctl",
        "issues": "https://github.com/mjanser/php-systemctl/issues"
    },
    "require": {
        "php": ">=7.0",
        "symfony/process": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": { "SystemCtl\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
